{
  "name": "muzika",
  "version": "2.0.0",
  "author": "Jakub Biesiada",
  "descripton": "React music player build with Spotify API. Muzika allows you search music in Spotify, display artists, albums, tracks with lyrics, playlist & more. App contains dark mode.",
  "license": "MIT",
  "private": true,
  "homepage": ".",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "next build",
    "start": "NODE_ENV=production ts-node server.ts",
    "prettier": "prettier --write '{src,public}/**/*.{tsx,html}'",
    "eslint": "eslint 'src/**/*.js' --fix",
    "commit": "git-cz"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/jest": "25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.2",
    "color-thief": "^2.2.5",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "js-cookie": "^2.2.1",
    "next": "^9.3.6",
    "next-compose-plugins": "^2.2.0",
    "next-cookies": "^2.0.3",
    "next-redux-wrapper": "^5.0.0",
    "night.js": "^1.2.0",
    "passport": "^0.4.1",
    "passport-spotify": "^1.1.0",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-debounce-input": "^3.2.2",
    "react-dom": "^16.13.1",
    "react-infinite-scroll-component": "^5.0.4",
    "react-is": "^16.13.1",
    "react-parlx": "^1.3.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-spinners": "^0.8.1",
    "react-viewport-hooks": "^1.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "string-strip-html": "^4.4.0",
    "styled-components": "4.4.1",
    "three": "^0.115.0",
    "two-digit": "^1.0.5",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/js-cookie": "^2.2.6",
    "@types/passport": "^1.0.3",
    "@types/react-redux": "^7.1.7",
    "@types/string-strip-html": "^4.3.0",
    "@types/styled-components": "^5.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cz-conventional-changelog": "3.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,public}/**/*.{tsx}": [
      "npm run prettier",
      "npm run eslint"
    ],
    "public/**/*.{html}": [
      "npm run prettier"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
