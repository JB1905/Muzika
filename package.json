{
  "name": "muzika",
  "version": "2.0.0",
  "author": "Jakub Biesiada",
  "descripton": "React music player build with Spotify API. Muzika allows you search music in Spotify, display artists, albums, tracks with lyrics, playlist & more. App contains dark mode.",
  "license": "MIT",
  "private": true,
  "homepage": ".",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write '{src,public}/**/*.{tsx,html}'",
    "eslint": "eslint 'src/**/*.js' --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/jest": "24.0.24",
    "@types/node": "12.12.21",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "color-thief": "^2.2.5",
    "dotenv": "^8.2.0",
    "next": "^9.1.6",
    "next-compose-plugins": "^2.2.0",
    "night.js": "^1.2.0",
    "passport": "^0.4.1",
    "passport-spotify": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-infinite-scroll-component": "^5.0.4",
    "react-parlx": "^1.3.0",
    "react-scripts": "3.3.0",
    "react-viewport-hooks": "^1.1.1",
    "string-strip-html": "^4.3.12",
    "styled-components": "^4.4.1",
    "typescript": "3.7.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.2",
    "@types/passport": "^1.0.2",
    "@types/string-strip-html": "^4.3.0",
    "@types/styled-components": "^4.4.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,public}/**/*.{tsx}": [
      "npm run prettier",
      "npm run eslint",
      "git add"
    ],
    "public/**/*.{html}": [
      "npm run prettier",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
